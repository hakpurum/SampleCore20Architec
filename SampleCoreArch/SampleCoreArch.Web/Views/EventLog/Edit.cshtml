            </div>
            <div class="form-group">
                <label asp-for="ResultObject.IpAdress" class="control-label"></label>
                <input data-bind="text:IpAdress" asp-for="ResultObject.IpAdress" class="form-control" />
                <span asp-validation-for="ResultObject.IpAdress" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ResultObject.HostName" class="control-label"></label>
                <input data-bind="text:HostName" asp-for="ResultObject.HostName" class="form-control" />
                <span asp-validation-for="ResultObject.HostName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ResultObject.CreatedTime" class="control-label"></label>
                <input data-bind="text:CreatedTime" asp-for="ResultObject.CreatedTime" class="form-control" />
                <span asp-validation-for="ResultObject.CreatedTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
                <input data-bind="click:Save" type="button" value="SaveKO" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script>
        model.EventLogEdit(@this.ViewContext.RouteData.Values["id"],function() { });
    </script>
}
